{
  "name": "immerser",
  "version": "4.0.4",
  "description": "Javascript library for switching fixed elements on scroll through sections. Like Midnight.js, but without jQuery",
  "source": "src/immerser.ts",
  "main": "dist/immerser.min.js",
  "exports": {
    ".": {
      "require": "./dist/immerser.min.js",
      "import": "./dist/immerser.min.js",
      "default": "./dist/immerser.min.js"
    }
  },
  "types": "./dist/immerser.min.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "keywords": [
    "scroll",
    "fixed",
    "sticky"
  ],
  "repository": "git@github.com:dubaua/immerser.git",
  "author": "Vladimir Lysov",
  "email": "dubaua@gmail.com",
  "license": "MIT",
  "homepage": "https://dubaua.github.io/immerser/",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@microsoft/api-extractor": "^7.55.1",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@zainulbr/i18n-webpack-plugin": "^2.0.3",
    "babel-loader": "^8.2.1",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.57.1",
    "file-loader": "^6.2.0",
    "gzip-size": "^6.0.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.6.0",
    "kind-of": "^6.0.3",
    "mini-css-extract-plugin": "^1.3.1",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.30.0",
    "sass": "^1.80.4",
    "sass-loader": "^13.3.2",
    "turndown": "^7.0.0",
    "typescript": "^5.8.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "scripts": {
    "build:lib": "webpack --mode production",
    "build:docs": "webpack --config ./webpack.config.docs.js --mode production",
    "build:options": "node scripts/build-options-table.js",
    "build:readme": "node scripts/readme.js",
    "build:docs:code": "node scripts/build-source-code.js",
    "build:post": "node scripts/post-build.js && npm run types",
    "build": "npm run lint && npm run build:options && npm run build:readme && npm run build:docs:code && npm run build:lib && npm run build:docs && npm run build:post",
    "types": "tsc --emitDeclarationOnly && api-extractor run --local",
    "dev": "webpack serve --config ./webpack.config.docs.js --mode development --open",
    "lint": "eslint ./src --ext .ts && eslint ./example/main.js"
  },
  "dependencies": {
    "@dubaua/merge-options": "^3.0.1",
    "@dubaua/observable": "^2.1.0"
  }
}
