{
  "name": "immerser",
  "version": "2.0.2",
  "description": "Javascript library for switching fixed elements on scroll through sections. Like Midnight.js, but without jQuery",
  "source": "src/immerser.js",
  "main": "dist/immerser.min.js",
  "umd:main": "dist/immerser.umd.js",
  "module": "dist/immerser.mjs",
  "keywords": [
    "scroll",
    "fixed",
    "sticky"
  ],
  "repository": "git@github.com:dubaua/immerser.git",
  "author": "Vladimir Lysov",
  "email": "dubaua@gmail.com",
  "license": "MIT",
  "homepage": "https://dubaua.github.io/immerser/",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "babel-loader": "^8.0.6",
    "create-observable": "^1.0.2",
    "css-loader": "^3.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "i18n-webpack-plugin": "^1.0.0",
    "microbundle": "^0.11.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prismjs": "^1.17.1",
    "rimraf": "^2.6.3",
    "sass-loader": "^8.0.0",
    "simplebar": "^4.0.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  },
  "scripts": {
    "build": "rimraf dist && microbundle --name Immerser --strict --external none",
    "docs": "rimraf docs && webpack --mode production",
    "dev": "webpack-dev-server --mode development --open"
  },
  "dependencies": {
    "@dubaua/merge-options": "^1.0.2"
  }
