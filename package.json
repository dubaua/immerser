  "homepage": "https://dubaua.github.io/immerser/",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@zainulbr/i18n-webpack-plugin": "^2.0.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.1",
    "css-loader": "^5.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.13.0",
    "eslint-config-standard": "^16.0.2",
    "file-loader": "^6.2.0",
    "gzip-size": "^6.0.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "kind-of": "^6.0.3",
